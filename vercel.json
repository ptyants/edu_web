{
	"version": 2,
	"builds": [
	  {
		"src": "app.py",
		"use": "@vercel/python"
	  }
	],
	"env": {
	  "_BARD_API_KEY": "your_bard_api_key_here",
	  "SECRET_KEY": "your_flask_secret_key_here",
	  "SQLALCHEMY_DATABASE_URI": "sqlite:///fact.db",
	  "UPLOAD_FOLDER": "static/uploads",
	  "ALLOWED_EXTENSIONS": "png,jpg,jpeg,gif,jfif"
	},
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "/app.py"
	  }
	]
  }